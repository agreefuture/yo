// return value: 0 means success, everything else failure

// TODO when we have a for...in statement, replace all individual if statements with loops testing for a wide range of numbers
fn main(): int {
    if 1 + 13 != 14 {
        ret 1;
    }

    if 1 - 17 != -16 {
        ret 1;
    }

    if 7 * 8 != 56 {
        ret 1;
    }

    if 81 / 9 != 9 {
        ret 1;
    }

    if 12 % 5 != 2 {
        ret 1;
    }

    if 13 & 1 != 1 {
        ret 1;
    }

    if 1 | 2 != 3 {
        ret 1;
    }

    if 5 ^ 7 != 2 {
        ret 1;
    }

    if 1 << 8 != 256 {
        ret 1;
    }

    if 512 >> 3 != 64 {
        ret 1;
    }

    ret 0;
}
