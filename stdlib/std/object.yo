// object

// every custom type you declare implicitly conforms to the `Object` protocol

protocol Object {
    fn hashValue(self: Self): int {
        ret self as int;
    }

    fn description(self: Self): String {
        ret String::format("<%s %nh>", [runtime::typeof(self), @(self as int)]);
    }

    //fn isEqual(self: Self, other: Self) {
    //    if self == other
    //}
}
