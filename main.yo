use "std/string";
use "std/runtime";
use "std/array";

fn double(x: int): int {
    ret x * 2;
}

fn main(): int {
    //val x: Array = [7, 8, 9];

    val x: Array = Array::new();
    x.add(1);
    x.add(2);
    x.add(3);

    io::printi(runtime::getRetainCount(x));
    runtime::retain(x);
    runtime::retain(x);
    io::printi(runtime::getRetainCount(x));

    runtime::release(x);
    runtime::release(x);
    io::printi(runtime::getRetainCount(x));

    ret x;

    //val y: Array = [double(1), double(2), double(3)];

}
