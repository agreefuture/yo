use "std/array";

fn f(): int {
    val x: int[];
    val y: int[];

    x = alloc(2);
    y = alloc(2);
    retain(x);
    retain(y);

    x[0] = 1;
    x[1] = 1;

    y[0] = 2;
    y[1] = 2;

    release(x);
    release(y);

    ret 0;
}


