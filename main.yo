use "std/array";

fn f(): int {
    val x: int[];
    val y: int[];

    x = runtime::alloc(2);
    y = runtime::alloc(2);
    runtime::retain(x);
    runtime::retain(y);

    x[0] = 1;
    x[1] = 1;

    y[0] = 2;
    y[1] = 2;

    runtime::release(x);
    runtime::release(y);

    ret 0;
}


